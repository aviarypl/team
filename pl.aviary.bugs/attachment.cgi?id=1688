error.login.reason.network    = Nie można połączyć się z serwerem
error.login.reason.synckey    = Nieprawidłowy klucz Sync
# error.login.reason.password is deprecated.
error.login.reason.password   = Nieprawidłowa nazwa użytkownika lub hasło
error.login.reason.account    = Nieprawidłowa nazwa konta lub hasło
error.login.reason.no_password= Brak zapisanego hasła
error.login.reason.no_synckey = Brak zapisanego klucza Sync
error.login.reason.server       = Nieprawidłowa konfiguracja serwera

error.sync.failed_partial     = Nie można zsynchronizować typów danych

invalid-captcha = Niewłaściwe słowa. Spróbuj ponownie.
weak-password   = Użyj hasła o większej sile

# this is the fallback, if we hit an error we didn't bother to localize
error.reason.unknown          = Nieznany błąd

change.synckey.sameAsSyncKey    = Nowy klucz nie może być taki sam, jak aktualny
change.synckey.sameAsPassword   = Klucz nie może być taki sam, jak hasło
change.synckey.sameAsUsername   = Klucz nie może być taki sam, jak nazwa użytkownika
change.synckey.sameAsEmail      = Klucz nie może być taki sam, jak adres e-mail użytkownika
change.synckey.tooShort         = Podany klucz jest za krótki

change.password.pwSameAsSyncKey      = Hasło nie może być takie samo, jak klucz
change.password.pwSameAsPassword     = Nowe hasło nie może być takie samo, jak aktualne hasło
change.password.pwSameAsUsername     = Hasło nie może być takie samo, jak nazwa użytkownika
change.password.pwSameAsEmail        = Hasło nie może być takie samo, jak twój adress e-mail
change.password.mismatch             = Podane hasła różnią się
change.password.tooShort             = Podane hasło jest za krótkie
